<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".TabReport">

    <Button
        android:id="@+id/btnNoise"
        android:layout_width="199dp"
        android:layout_height="0dp"
        android:layout_marginTop="228dp"
        android:layout_marginBottom="50dp"
        android:text="시끄러운 사람이 있어요!"
        app:layout_constraintBottom_toTopOf="@+id/btnPseudo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPseudo"
        android:layout_width="199dp"
        android:layout_height="0dp"
        android:layout_marginBottom="50dp"
        android:text="사이비가 있어요!"
        app:layout_constraintBottom_toTopOf="@+id/btnShopkeeper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnNoise" />

    <Button
        android:id="@+id/btnShopkeeper"
        android:layout_width="199dp"
        android:layout_height="0dp"
        android:layout_marginBottom="203dp"
        android:text="누가 물건을 팔아요!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPseudo" />
</androidx.constraintlayout.widget.ConstraintLayout>